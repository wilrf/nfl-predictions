{
  "version": 2,
  "name": "nfl-betting-system",
  "buildCommand": "cd improved_nfl_system/web && pip install -r ../requirements.txt",
  "devCommand": "cd improved_nfl_system && python web/launch.py",
  "installCommand": "pip install -r improved_nfl_system/requirements.txt",
  "framework": null,
  "outputDirectory": "improved_nfl_system/web",
  "public": false,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "env": {
    "ODDS_API_KEY": "@odds-api-key"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  }
}
