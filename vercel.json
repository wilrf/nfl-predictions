{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/health.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/predictions.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/stats.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/games.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/confidence_analysis.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/weekly_performance.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/weekly_performance",
      "dest": "api/weekly_performance.py"
    },
    {
      "src": "/api/stats",
      "dest": "api/stats.py"
    },
    {
      "src": "/api/games",
      "dest": "api/games.py"
    },
    {
      "src": "/api/confidence_analysis",
      "dest": "api/confidence_analysis.py"
    },
    {
      "src": "/api/predictions",
      "dest": "api/predictions.py"
    },
    {
      "src": "/api/health",
      "dest": "api/health.py"
    },
    {
      "src": "/api",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ]
}
